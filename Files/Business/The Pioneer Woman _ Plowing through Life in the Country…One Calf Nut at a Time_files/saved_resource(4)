(function($){

  // Variables used in all the following sections.
  var $window = $(window),
    $body = $('body'),
    $html = $('html'),
    wpAdminBar = $body.hasClass('admin-bar'),
    htmlMarginTop = function() { return parseInt( $html.css('margin-top') ) },
    mobileMax = 767,
    desktopMin = 992;

  var $pageHeader = $('#page-header'),
    $headerImage = $pageHeader.find('.header-image');

  // Retina check
  function isRetina() {
    var mediaQuery = "(-webkit-min-device-pixel-ratio: 1.5),\
            (min--moz-device-pixel-ratio: 1.5),\
            (-o-min-device-pixel-ratio: 3/2),\
            (min-resolution: 1.5dppx)";
    if (window.devicePixelRatio > 1) {
      return true;
    }
    return (window.matchMedia && window.matchMedia(mediaQuery).matches);
  }

  // Set the proper background-image on header-image image-* elements.
  function setRetinaHeaderImage() {

    if (!isRetina()) {
      return false;
    }

    var isMobile = mobileMax >= $window.width(),
      leftImgUrlRetina = $headerImage.data(isMobile ? 'bg-mobile-retina' : 'bg-desktop-left-retina'),
      rightImgUrlRetina = $headerImage.data(isMobile ? 'bg-mobile-retina' : 'bg-desktop-right-retina'),
      repeatingImgUrlRetina = $headerImage.data(isMobile ? '' : 'bg-repeating-retina');

    if ('undefined' !== typeof leftImgUrlRetina && '' !== leftImgUrlRetina) {
      $headerImage.find('.image-left').css('background-image', 'url(' + leftImgUrlRetina + ')');
    }
    if ('undefined' !== typeof rightImgUrlRetina && '' !== rightImgUrlRetina) {
      $headerImage.find('.image-right').css('background-image', 'url(' + rightImgUrlRetina + ')');
    }
    if ('undefined' !== typeof repeatingImgUrlRetina && '' !== repeatingImgUrlRetina) {
      $headerImage.css('background-image', 'url(' + repeatingImgUrlRetina + ')');
    }
  }

  setRetinaHeaderImage();
  $window.on('resize', setRetinaHeaderImage);

})(jQuery);;
